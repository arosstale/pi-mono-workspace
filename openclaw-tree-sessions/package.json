{
  "name": "@openclaw/tree-sessions",
  "version": "1.0.0",
  "description": "Tree-structured session history for OpenClaw with branching conversations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "openclaw",
    "tree-structured",
    "session-history",
    "branching",
    "conversation-tree",
    "ai-chat"
  ],
  "author": "OpenClaw Team",
  "license": "MIT",
  "dependencies": {
    "uuid": "^10.0.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "@types/express": "^4.17.21",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "files": [
    "dist",
    "README.md",
    "TREE_SESSIONS.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/openclaw/tree-sessions"
  },
  "bugs": {
    "url": "https://github.com/openclaw/tree-sessions/issues"
  },
  "homepage": "https://github.com/openclaw/tree-sessions#readme"
}
